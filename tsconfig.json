        "jspm_packages/npm/angular2@2.0.0-alpha.44/bundles/typings/angular2/testing.d.ts",
        "jspm_packages/npm/angular2@2.0.0-alpha.44/bundles/typings/es6-shim/es6-shim.d.ts",
        "jspm_packages/npm/angular2@2.0.0-alpha.44/bundles/typings/jasmine/jasmine.d.ts",
