{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "build",
        "sourceMap": true
    },
    "filesGlob": [
        "!node_modules/**/*.ts",
        "!jspm_packages/**/*.ts",
        "jspm_packages/**/bundles/**/*.d.ts",
        "src/**/*.ts",
        "typings/**/*.d.ts"
    ],
